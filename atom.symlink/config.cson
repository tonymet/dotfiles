"*":
  editor:
    showIndentGuide: true
    fontFamily: "Menlo"
    invisibles: {}
    fontSize: 15
    softWrapAtPreferredLineLength: true
  whitespace:
    removeTrailingWhitespace: false
    ensureSingleTrailingNewline: false
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    themes: [
      "unity-ui"
      "glacier-syntax"
    ]
    hideGitIgnoredFiles: true
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "atom-jshint"
    ]
    excludeVcsIgnoredPaths: false
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  welcome:
    showOnStartup: false
  "release-notes":
    viewedVersion: "0.89.0"
  "project-quick-open":
    projectPaths: "~/GoogleDrive/Documents/workspace/,/Users/tonym/insomniac"
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode": {}
